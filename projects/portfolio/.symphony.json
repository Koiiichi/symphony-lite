{
  "cli_options": {
    "detailed_log": false,
    "max_passes": 3,
    "open_browser": true,
    "vision_mode": "hybrid"
  },
  "start_commands": {
    "backend": {
      "command": [
        "python",
        "app.py"
      ],
      "cwd": "backend",
      "description": "Flask backend API",
      "kind": "backend",
      "port": 5001
    },
    "frontend": {
      "command": [
        "python",
        "-m",
        "http.server",
        "8000"
      ],
      "cwd": "frontend",
      "description": "Static frontend server",
      "kind": "frontend",
      "port": 8000,
      "url": "http://localhost:8000/index.html"
    }
  }
}